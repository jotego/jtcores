CORENAME=JTSIMSON

JTFRAME_STEREO
# Video configuration
JTFRAME_COLORW=8
JTFRAME_PXLCLK=6
JTFRAME_WIDTH=320
JTFRAME_HEIGHT=224

JTFRAME_CREDITS
JTFRAME_CREDITS_PAGES=4
JTFRAME_CREDITS_HSTART=32

JTFRAME_JOY_DURL
JTFRAME_BUTTONS=3

# ROM configuration
JTFRAME_HEADER = 4
JTFRAME_BA1_START =0x80000
PCM_START         =0x0A0000
JTFRAME_BA2_START =0x1E0000
JTFRAME_BA3_START =0x2E0000
# consider using 4 byte bursts as objects will benefit:
#JTFRAME_BA3_LEN   = 64
JTFRAME_PROM_START=0x6E0000

JTFRAME_IOCTL_RD=128
JTFRAME_OSD_TEST
# JTFRAME_INPUT_RECORD

# debugging content: (128 +) 24 kB + 16
debug JTFRAME_IOCTL_RD+=28705

[mist|sidi]
