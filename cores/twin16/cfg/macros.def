JTFRAME_COLORW=8
JTFRAME_PXLCLK=6
JTFRAME_WIDTH=320
JTFRAME_HEIGHT=224
JTFRAME_PLL=jtframe_pll6144
JTFRAME_BA1_WEN
# ROM configuration
JTFRAME_HEADER=8
