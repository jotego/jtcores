[all]
CORENAME=JTKIWI
JTFRAME_BUTTONS=3
JTFRAME_CREDITS
JTFRAME_JOY_DURL

# Extermination, Kageki, etc. are vertical games
JTFRAME_VERTICAL
JTFRAME_CREDITS_HIDEVERT

# ROM loading
BRAM_OFFSET=0x100000
JTFRAME_HEADER=2
JTFRAME_BA1_START=0x20000
JTFRAME_BA2_START=0x30000
PCM_START=0x40000
JTFRAME_BA3_START=0x50000
JTFRAME_PROM_START=0x250000
MCU_START=0x251000

# Video
JTFRAME_COLORW=5
JTFRAME_WIDTH=256
JTFRAME_HEIGHT=224

JTFRAME_OSD_SND_EN
JTFRAME_OSD_VOL

# Debug options
JTFRAME_STATUS
JTFRAME_DEBUG

# Clocks
JTFRAME_PXLCLK=6

[mister]
JTFRAME_ARX=5
JTFRAME_ARY=4

[mist|sidi]
# The OSD seems to get broken. Some firmware problem?
-JTFRAME_OSD_SND_EN
-JTFRAME_OSD_VOL
# No enough BRAM for everything, after the 32kB of RAM
# for tnzs was added
-JTFRAME_CREDITS
-JTFRAME_OSD_NOLOGO