[*]
# support for core names not starting with JT
# seems incomplete at the moment
CORENAME=JTTOKI
# GAMETOP=jttoki_game_sdram

### Screen parameters

JTFRAME_CREDITS
JTFRAME_COLORW=4
JTFRAME_PXLCLK=6
JTFRAME_HEIGHT=224
JTFRAME_WIDTH=256

### Audio section

JTFRAME_OSD_SND_EN
JTFRAME_OSD_VOL

### PCB Configuration

JTFRAME_BUTTONS=2

JTFRAME_CLK48
JTFRAME_SDRAM96

JTFRAME_BA1_START =0x280000
#start offset of 2nd bank buses in bytes 

[mist|sidi]
# won't fit
JTFRAME_SKIP

[mister]
JTFRAME_ARX=4
JTFRAME_ARY=3

