CORENAME=JTSHOUSE

JTFRAME_STEREO
JTFRAME_BUTTONS=6
JTFRAME_DIAL

# Video configuration
JTFRAME_COLORW=8
JTFRAME_PXLCLK=6
JTFRAME_WIDTH=288
JTFRAME_HEIGHT=224
JTFRAME_VERTICAL
JTFRAME_PLL=jtframe_pll6144
JTFRAME_ARX=4
JTFRAME_ARY=3
# ROM configuration
