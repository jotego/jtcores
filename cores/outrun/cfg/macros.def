CORENAME=JTOUTRUN
GAMETOP=jtoutrun_game_sdram

JTFRAME_PXLCLK=6

JTFRAME_BUTTONS=4
JTFRAME_ANALOG
JTFRAME_ANALOG_DUAL
JTFRAME_STEREO

# This game is skipped for
# all platforms except Pocket, MiSTer & family and NeptUNO
JTFRAME_SKIP

# The decrypter module is
# omitted by default to save BRAM
NODEC

JTFRAME_HEADER=16
# Use 64-bit bursts to speed up graphics
JTFRAME_BA0_LEN=64
JTFRAME_BA2_LEN=64
JTFRAME_BA3_LEN=64

# Screen parameters
JTFRAME_PLL=jtframe_pll6293
JTFRAME_COLORW=5
JTFRAME_WIDTH=320
JTFRAME_HEIGHT=224

# Debug
JTFRAME_DEBUG
JTFRAME_STATUS
JTFRAME_IOCTL_RD=18432

# Clock domains
JTFRAME_CLK48

# OSD options
JTFRAME_OSD_VOL
JTFRAME_OSD_SND_EN
JTFRAME_OSD_TEST

# ROM Regions
JTFRAME_BA1_START    = 0x060000
SND_START = 0x0C0000
PCM_START = 0x0D0000
JTFRAME_BA2_START = 0x130000
JTFRAME_BA3_START = 0x170000
ROAD_START   = 0x270000
JTFRAME_PROM_START = 0x298000
FD1089_START = 0x29A000
JTFRAME_OSD_NOLOGO

# Make it the same as KEY_START
JTFRAME_PROM_START=0x298000

[pocket|mister|sockit|de1*|neptuno|demist_zx3a?00|mist_psd_gx150]
-JTFRAME_SKIP
# Take clock down to 50MHz and use the
# line-based frame buffer for objects
JTFRAME_LF_BUFFER
-JTFRAME_OSD_NOLOGO
-NODEC
JTFRAME_CREDITS
JTFRAME_CREDITS_PAGES=3
JTFRAME_CREDITS_HSTART=32

[demist_zx3a35]
-JTFRAME_SKIP
JTFRAME_LF_BUFFER
JTFRAME_OSD_NOLOGO
NODEC
JTFRAME_OSD_NOCREDITS
-JTFRAME_CREDITS

[mister|sockit|de1soc|de10std]
# Controller options have only been tested on MiSTer
CORE_OSD=OKM,Control Type,A.Stick,A.Triggers,A.Wheel;
#JTFRAME_CHEAT

